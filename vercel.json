{
    "version": 2,
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "./(.*)",
            "dest": "/"
        },
        {
            "src": "/all",
            "dest": "/index.js"
        }
    ]
}